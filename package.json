{
  "name": "crypt-wrapper",
  "version": "1.0.11",
  "description": "Encrypt/Decrypt Wrapper Utility",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": "^16.10.0 || ^18.0.0 || latest"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "start": "npm run build && node .",
    "build": "tsc",
    "test": "cross-env jest --config jestconfig.json --verbose",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saivirtue/crypt-wrapper.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "crypto",
    "node"
  ],
  "author": "Sam CH Cho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saivirtue/crypt-wrapper/issues"
  },
  "homepage": "https://github.com/saivirtue/crypt-wrapper#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21"
  },
  "engineStrict": true,
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.191",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  }
}
